{
	"name": "<<name>>",
	"version": "<<version>>",
	"manifest_version": 2,
	"description": "<<description>>",
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"64": "icon64.png",
		"128": "icon128.png"
	},
	"content_scripts": [{
		"js": ["<<script>>"],
		"matches": <<json:urls>>,
		"all_frames": true,
		"run_at": "document_start"
	}],
	"homepage_url": "<<homepage>>",
	"permissions": [
		"storage"
	]
}